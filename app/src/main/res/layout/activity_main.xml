<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data></data>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/activity_main_change_status_bar_color"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="change status bar color"
            />

        <Button
            android:id="@+id/activity_main_change_status_bar_text_color"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="change status bar text color" />

        <android.support.v7.widget.RecyclerView xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/activity_main_webp_demo_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context="com.coofee.webpdemo.MainActivity" />
    </LinearLayout>

</layout>

